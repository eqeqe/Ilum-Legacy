ulTab:AddParagraph("6/6/22","Initial gui created ðŸ˜…")

local crTab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
crTab:AddParagraph("Coding","Ilum scripts by silvrem; silvrem#8499")
